{
  "name": "seemusic-ui",
  "version": "1.0.0",
  "description": "A Vue 3 UI Library. Uses Composable.",
  "keywords": [
    "seemusic-ui",
    "ui library",
    "vue"
  ],
  "license": "MIT",
  "author": "xsq",
  "scripts": {
    "build:dist": "vite build -c vite.dist-min.config.ts && vite build -c vite.dist.config.ts",
    "build:lib": "vite build -c vite.lib.config.ts",
    "build": "pnpm run build:lib && pnpm run build:dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "lib",
    "dist",
    "assets"
  ],
  "main": "lib/framework.mjs",
  "module": "lib/framework.mjs",
  "types": "lib/framework.d.ts",
  "exports": {
    ".": {
      "import": "./lib/framework.mjs",
      "require": "./dist/seemusic-ui.js"
    },
    "./components": "./lib/components/index.mjs",
    "./components/*": "./lib/components/*/*.mjs",
    "./framework": "./lib/framework.mjs",
    "./styles": "./lib/styles/main.scss",
    "./styles/*": "./lib/styles/*",
    "./utils": "./lib/utils/index.mjs",
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "lib/framework.mjs": [
        "lib/framework.d.ts"
      ],
      "framework": [
        "lib/framework.d.ts"
      ],
      "*": [
        "*",
        "dist/*",
        "lib/*",
        "lib/*.d.ts",
        "lib/*/index.d.ts"
      ]
    }
  },
  "peerDependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@seemusic/element-plus-theme-sop": "workspace:^2.2.28-rc.2",
    "@storybook/addon-docs": "7.0.0-beta.43",
    "@storybook/addon-essentials": "^7.0.0-beta.43",
    "@storybook/addon-interactions": "^7.0.0-beta.43",
    "@storybook/addon-links": "^7.0.0-beta.43",
    "@storybook/blocks": "^7.0.0-beta.43",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/vue3": "^7.0.0-beta.43",
    "@storybook/vue3-vite": "^7.0.0-beta.43",
    "@types/mkdirp": "^1.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "element-plus": "^2.2.27",
    "magic-string": "^0.27.0",
    "mkdirp": "^1.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.43",
    "typescript": "~4.7.4",
    "vite-plugin-dts": "^1.7.1",
    "vue-router": "^4.1.6"
  }
}
